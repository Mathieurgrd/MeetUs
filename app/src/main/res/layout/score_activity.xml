<RelativeLayout android:id="@+id/activity_score"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.mathieu.meetus.ScoreResultsActivity"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ImageView
        android:id="@+id/imageView7"
        android:layout_alignBottom="@+id/imageView12"
        android:layout_below="@+id/textView7"
        android:layout_centerHorizontal="true"
        android:layout_height="wrap_content"
        android:layout_marginTop="38dp"
        android:layout_width="wrap_content"
        app:srcCompat="@drawable/start" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_alignTop="@+id/imageView3"
        android:layout_centerHorizontal="true"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:layout_width="wrap_content"
        app:srcCompat="@drawable/dix" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_alignEnd="@+id/imageView7"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@+id/imageView7"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:layout_marginEnd="14dp"
        android:layout_marginRight="14dp"
        android:layout_width="wrap_content"
        app:srcCompat="@drawable/start" />

    <TextView
        android:id="@+id/textView7"
        android:layout_above="@+id/imageView12"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_height="wrap_content"
        android:layout_marginBottom="46dp"
        android:layout_width="wrap_content"
        android:text="Tu as l impression de bien connaitre Marine mais essaie encore ça peut être mieux !" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_above="@+id/textView7"
        android:layout_centerHorizontal="true"
        android:layout_height="100dp"
        android:layout_marginBottom="13dp"
        android:layout_width="100dp"
        app:srcCompat="@drawable/oval" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_above="@+id/imageView5"
        android:layout_centerHorizontal="true"
        android:layout_height="wrap_content"
        android:layout_marginBottom="13dp"
        android:layout_width="wrap_content"
        app:srcCompat="@drawable/photoflou" />

    <TextView
        android:id="@+id/textView5"
        android:layout_centerHorizontal="true"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="BRAVO !"
        android:textSize="20dp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:srcCompat="@drawable/quest"
        android:layout_alignBottom="@+id/imageView3"
        android:layout_alignRight="@+id/textView5"
        android:layout_alignEnd="@+id/textView5" />

    <Button
        android:background="@drawable/start"
        android:id="@+id/button2"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="TRY AGAIN"
        android:textColor="#FEFEFE"
        android:textSize="18dp"
        android:layout_below="@+id/textView7"
        android:layout_alignLeft="@+id/imageView12"
        android:layout_alignStart="@+id/imageView12" />

</RelativeLayout>